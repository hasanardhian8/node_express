{
  "name": "codeidaca-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --exec babel-node ./server/index.js",
    "clean": "rm -rf build && mkdir build",
    "cleanw": "rmdir /S build && mkdir build",
    "start": "node ../server/index.js",
    "build-server": "babel -d ./build ./server -s",
    "build": "npm run clean && npm run build-server",
    "buildw": "npm run cleanw && npm run build-server",
    "test": "jest",
    "sql": "npx sequelize-cli db:seed:all",
    "reverse:db": "sequelize-auto -o ./schemas -d codeiddb -h localhost -u postgres -p 5432 -x admin -e postgres -l esm"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.19.2",
    "express-pino-logger": "^6.0.0",
    "express-session": "^1.18.0",
    "formidable": "^2.1.2",
    "graphql": "^15.8.0",
    "graphql-tools": "^8.3.20",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.5",
    "pino-noir": "^2.2.1",
    "sequelize": "^5.22.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/core": "^7.24.5",
    "@babel/node": "^7.23.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.5",
    "@babel/runtime": "^7.24.5",
    "@types/dotenv": "^8.2.0",
    "nodemon": "^2.0.22",
    "sequelize-auto": "^0.8.8"
  }
}
